{
  "name": "damascus-master",
  "version": "1.0.0",
  "description": "Damascus Master E-commerce Website",
  "main": "server/server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.2.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "stripe": "^11.11.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "multer": "^1.4.5",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.0",
    "zod": "^3.22.4",
    "lowdb": "^6.0.1",
    "nodemailer": "^6.9.8"
  },
  "scripts": {
    "start": "NODE_ENV=production node server/server.js",
    "dev": "nodemon --watch server --ignore server/data/db.json server/server.js",
    "build": "echo 'Build completed - static files ready'",
    "test:embed": "node server/lib/embed.test.cjs",
    "test": "npm run test:embed",
    "clean": "node cleanup_albums.js",
    "watch:clean": "nodemon --watch server/data/db.json --exec \"node cleanup_albums.js\""
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "nodemon": "^3.1.10"
  },
  "keywords": ["ecommerce", "damascus", "knives", "nodejs", "express"],
  "author": "Damascus Master",
  "license": "MIT"
}
